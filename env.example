# Configurações do Banco de Dados
POSTGRES_PASSWORD=your-secure-password-here

# Configurações do PgAdmin
PGADMIN_EMAIL=admin@automo.com
PGADMIN_PASSWORD=your-secure-password-here

# Configurações da Aplicação
IMAGE=automo-app
TAG=latest
HOST_PORT=8081

# Configurações de Email
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=no-reply@automo.com
SMTP_PASSWORD=your-app-password-here

# Configurações de JWT
JWT_SECRET_KEY=your-very-long-and-secure-jwt-secret-key-here
JWT_EXPIRATION=86400000
JWT_REFRESH_EXPIRATION=604800000

# Configurações de SMS
SMS_ENABLED=false
SMS_API_KEY=your-sms-provider-api-key
SMS_SENDER=AUTOMO

# Configurações de Storage
UPLOAD_FOLDER=./storage 